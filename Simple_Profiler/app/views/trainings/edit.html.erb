<% provide(:title, "Edit") %>
<h1>Training#edit</h1>
<div class=" row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @training, url: edit_training_path(@training) do |f| %>
      <%= render 'trainings/shared/error_messages' %>
      <%= render :partial => 'trainings/shared/fields', :locals => { :f => f } %>
      <div class="row">
        <div class="col-xs-12 col-md-6"><%= f.submit 'Submit', class: 'btn btn-primary' %></div>
      </div>
    <% end %>
  </div>
</div>
