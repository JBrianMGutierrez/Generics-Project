<% provide(:title, "Report a Scene")%>
<div class="jumbotron" style="background: rgba(245,245,245,0.7)">
  <div class="row">

    <div class="col-md-8 col-md-offset-2">
      <%= form_for @reports, url: reports_new_path do |f| %>
        <%= render 'reports/shared/error_messages' %>
        <%= render :partial => 'reports/shared/fields', :locals => { :f => f } %>
      <% end %>
    </div>
  </div>
</div>
